<div id="breadcrumb" class="breadCrumb">
	<ul>
		{{#if previousRoutes}}		
			{{#each previousRoutes}}
				<li>
					<a href="#"{{action "gotoRoute" this}}>{{this}}</a>
				</li>
			{{/each}}
			<li>
				{{currentRoute}}
			</li>
		{{else}}
			<li>
				<a href="#">{{currentRoute}}</a>
			</li>
		{{/if}}
	</ul>
</div>
	

